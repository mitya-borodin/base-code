    "moment": "^2.29.1",
    "mongodb": "^3.6.4",
    "ms": "^2.1.3",
    "type-is": "^1.6.18",
    "uuid": "^8.3.2",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@rtcts/isomorphic": "^1.6.0",
    "@rtcts/utils": "^1.6.0",
