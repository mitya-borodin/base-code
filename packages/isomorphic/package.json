{
  "version": "1.6.1",
  "name": "@rtcts/isomorphic",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://github.com/mitya-borodin/rtcts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:mitya-borodin/rtcts.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://github.com/mitya-borodin/rtcts/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "entity",
    "framework",
    "isomorphic",
    "library",
    "log",
    "rearguard",
    "user",
    "validates",
    "value object",
    "ws"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/isomorphic.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json",
    "fmt": "prettier --write **",
    "lint": "tsdx lint src test --fix",
    "test": "tsdx test",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "prepare": "tsdx build --tsconfig tsconfig.build.json",
    "outdated": "npm-check-updates",
    "upgrade": "npm-check-updates -u --target minor"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --verbose"
    }
  },
  "lint-staged": {
    "*.(ts|js|json)": [
      "pretty-quick --staged --pattern **/*.{ts,js,json} --verbose"
    ],
    "src/**/*.(ts|js)": [
      "npm run lint"
    ],
    "test/**/*.(ts|js)": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@rtcts/utils": "^1.6.1",
    "@types/jest": "^26.0.20",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.25",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "npm-check-updates": "^11.1.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
