{
  "name": "@borodindmitriy/isomorphic",
  "description": "Isomorphic common classes's library.",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "bugs": {
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "version": "1.0.21",
  "main": "lib/lib_exports.js",
  "module": "lib/lib_exports.js",
  "files": [
    "lib",
    "lib_bundle"
  ],
  "scripts": {
    "build": "rearguard build --ui_lib --node_lib",
    "build:both": "rearguard build --both --ui_lib --node_lib",
    "build:release": "rearguard build -r --ui_lib --node_lib",
    "prepublishOnly": "npm run build:both",
    "start": "rearguard wds",
    "start:release": "rearguard wds -r"
  },
  "dependencies": {},
  "peerDependencies": {
    "@borodindmitriy/interfaces": "1.0.16",
    "@borodindmitriy/utils": "1.3.18",
    "@borodindmitriy/vendors": "1.0.7"
  },
  "devDependencies": {
    "@borodindmitriy/interfaces": "1.0.16",
    "@borodindmitriy/utils": "1.3.18",
    "@borodindmitriy/vendors": "1.0.7"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "rearguard": {
    "context": "src",
    "entry": "index.tsx",
    "lib_entry": "lib_exports.ts",
    "modules": [
      "src"
    ],
    "output": {
      "path": "dist",
      "publicPath": "/"
    },
    "sync_project_deps": [
      "@borodindmitriy/interfaces",
      "@borodindmitriy/vendors",
      "@borodindmitriy/utils"
    ],
    "has_dll": false,
    "has_ui_lib": true,
    "load_on_demand": false,
    "has_project": false,
    "has_node_lib": true,
    "publish_in_git": false
  },
  "types": "lib/lib_exports.d.ts"
}