{
  "name": "@rtcts/isomorphic",
  "description": "Isomorphic part of the framework, contains basic abstractions and implementations",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/mitya-borodin/base-code.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "version": "1.1.6",
  "main": "./lib/export.js",
  "bin": "",
  "files": [
    "lib",
    "lib_bundle"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "rearguard start",
    "start:ts_node_dev": "rearguard start --ts_node_dev",
    "build": "rearguard build",
    "test": "rearguard test",
    "lint": "rearguard lint",
    "lint-fix": "rearguard lint --fix",
    "typecheck": "tsc --noEmit",
    "validate-prettiness": "prettier -c '**/*.{ts,tsx,json,md}'",
    "validate": "npm run lint && npm run typecheck && npm run validate-prettiness",
    "make-prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "sync": "rearguard sync",
    "refresh": "rearguard refresh",
    "watch": "rearguard watch"
  },
  "types": "./lib/export.d.ts",
  "dependencies": {
    "@rtcts/utils": "1.1.6",
    "@rtcts/vendor-other": "1.1.6",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "tslib": "2.0.0"
  },
  "devDependencies": {
    "@types/eslint": "6.1.3",
    "@types/lodash.omit": "4.5.6",
    "@types/lodash.pick": "4.4.6",
    "@types/prettier": "1.19.0",
    "@types/stylelint": "9.10.1",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "husky": "4.2.5",
    "normalize.css": "8.0.1",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "stylelint": "13.5.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "typescript": "3.9.3"
  },
  "keywords": [
    "entity",
    "framework",
    "isomorphic",
    "library",
    "log",
    "rearguard",
    "user",
    "validates",
    "value object",
    "ws"
  ],
  "engines": {
    "node": ">=10 <13",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
  },
  "rearguard": {
    "webpack": {
      "context": "src",
      "dll_entry": "rtcts_isomorphic_vendors.ts",
      "entry": "index.tsx",
      "lib_entry": "export.ts",
      "modules": [
        "src"
      ],
      "output": {
        "path": "dist",
        "publicPath": "/",
        "libraryTarget": "var"
      }
    },
    "html": {
      "noInjectAssets": false
    },
    "css": {
      "postcssPlugins": "postcss.config.js"
    },
    "project": {
      "runtime": "isomorphic",
      "type": "lib",
      "thisModuleWillLoadOnDemand": false,
      "buildListOfLoadOnDemandModulesForAll": false,
      "unPublishedDependency": []
    },
    "configs": {
      "noOverwriteTSConfig": false,
      "noOverwriteTSTestConfig": false,
      "noOverwriteLintConfig": false,
      "noOverwriteGitIgnore": false
    },
    "distribution": {
      "publish_to_git": false
    }
  }
}
