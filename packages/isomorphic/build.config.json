{
  "context": "src",
  "entry": "index.tsx",
  "lib_entry": "lib_exports.ts",
  "dll_entry": "vendors.ts",
  "modules": ["src"],
  "output": {
    "path": "dist",
    "publicPath": "/"
  },
  "typescript": {
    "config": {
      "compileOnSave": false,
      "compilerOptions": {},
      "exclude": [],
      "include": []
    },
    "configPath": "tsconfig.json"
  },
  "postCSS": {
    "plugins": "postCssPlugins.js"
  },
  "proxy": {
    "/api": "http://localhost:5000",
    "/ws": {
      "changeOrigin": true,
      "target": "http://localhost:5000",
      "ws": true
    }
  },
  "sync_npm_deps": ["@borodindmitriy/vendors-dll", "@borodindmitriy/interfaces", "@borodindmitriy/utils"]
}
