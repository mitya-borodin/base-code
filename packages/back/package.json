    "del-cli": "1.1.0",
    "express": "4.16.4",
    "husky": "1.3.1",
    "mocha": "6.0.2",
    "moment": "2.24.0",
    "mongodb": "3.1.13",
    "nyc": "13.3.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
