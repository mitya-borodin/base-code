{
  "name": "@borodindmitriy/back-end-lib",
  "description": "Back-end common classes's library.",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "version": "3.0.5",
  "main": "./lib/export.ts",
  "bin": "",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npx rearguard build",
    "test": "npx rearguard test",
    "sync": "npx rearguard sync",
    "check_deps_on_npm": "npx rearguard check_deps_on_npm",
    "lint": "npx rearguard lint",
    "lint-fix": "npx rearguard lint --fix",
    "typecheck": "tsc --noEmit",
    "validate-prettiness": "prettier -c '**/*.{ts,tsx,json,md}'",
    "validate": "npm run lint && npm run typecheck && npm run validate-prettiness",
    "make-prettier": "prettier --write '**/*.{ts,tsx,json,md}'"
  },
  "types": "./lib/export.d.ts",
  "dependencies": {
    "@borodindmitriy/interfaces": "3.0.5",
    "@borodindmitriy/isomorphic": "3.0.4",
    "@borodindmitriy/utils": "3.0.4",
    "body-parser": "1.18.3",
    "bson": "3.0.2",
    "chalk": "2.4.2",
    "express": "4.16.4",
    "moment": "2.24.0",
    "mongodb": "3.1.13",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "tslib": "1.10.0",
    "uuid": "3.3.2",
    "ws": "6.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/bson": "1.0.11",
    "@types/chai": "4.1.7",
    "@types/express": "4.16.1",
    "@types/mocha": "5.2.6",
    "@types/mongodb": "3.1.22",
    "@types/node": "12.7.12",
    "@types/passport": "1.0.0",
    "@types/passport-jwt": "3.0.1",
    "@types/uuid": "3.4.4",
    "@types/ws": "6.0.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "husky": "1.3.1",
    "mocha": "6.0.2",
    "nyc": "14.1.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "8.0.2",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.14.0",
    "tslint-no-circular-imports": "0.6.2",
    "typescript": "3.6.2"
  },
  "engines": {
    "node": ">=10 <11",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
  },
  "rearguard": {
    "project": {
      "runtime": "node",
      "type": "lib"
    },
    "distribution": {
      "publish_to_git": false
    },
    "configs": {
      "noOverwriteTSConfig": false,
      "noOverwriteTSTestConfig": false,
      "noOverwriteLintConfig": false,
      "noOverwriteGitIgnore": false
    }
  }
}
