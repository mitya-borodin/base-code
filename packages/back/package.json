{
  "name": "@borodindmitriy/back-end-library",
  "description": "Common classes's library for back-end.",
  "version": "1.0.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "main": "lib",
  "module": "lib",
  "types": "lib",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "bugs": {
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "author": {
    "name": "Dmitriy Borodin",
    "email": "dmitriy@borodin.site",
    "url": "http://borodin.site"
  },
  "scripts": {
    "build": "rearguard build --lib && rearguard build --lib -r",
    "build:relink": "npm run build && npm link",
    "sync": "rearguard sync_deps",
    "test": "mocha --require ./tests/setup.js tests/unit/**/*.spec.ts",
    "test:coverage": "nyc --extension .ts --reporter=html mocha --require ./tests/setup.js tests/unit/**/*.spec.ts",
    "test:watch": "mocha -R progress --require ./tests/setup.js --watch-extensions ts --watch tests/unit/**/*.spec.ts"
  },
  "devDependencies": {
    "@types/bson": "1.0.11",
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "@types/mongodb": "3.1.12",
    "@types/node": "10.12.0",
    "@types/passport": "0.4.6",
    "@types/passport-jwt": "3.0.1",
    "@types/uuid": "3.4.4",
    "@types/ws": "6.0.1",
    "body-parser": "1.18.3",
    "bson": "3.0.2",
    "chai": "4.2.0",
    "chalk": "2.4.1",
    "del-cli": "1.1.0",
    "express": "4.16.4",
    "husky": "1.1.2",
    "mocha": "5.2.0",
    "moment": "2.22.2",
    "mongodb": "3.1.8",
    "nyc": "13.1.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "prettier": "1.14.3",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "tslint-no-circular-imports": "0.6.1",
    "typescript": "3.1.3",
    "uuid": "3.3.2",
    "ws": "6.1.0"
  },
  "peerDependencies": {
    "moment": "2.22.2",
    "tslib": "1.9.3",
    "bson": "3.0.2",
    "passport-jwt": "4.0.0",
