{
  "name": "@borodindmitriy/deferred_module_test",
  "description": "Deferred_module_test.",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "bugs": {
    "url": "https://gitlab.com/mitya-borodin/base-code/issues"
  },
  "version": "1.0.24",
  "main": "lib/lib_exports.js",
  "module": "lib/lib_exports.js",
  "files": [
    "dll_bundle",
    "lib",
    "lib_bundle"
  ],
  "scripts": {
    "build": "rearguard build --dll --browser_lib",
    "build:both": "rearguard build --both --dll --browser_lib",
    "build:release": "rearguard build -r --dll --browser_lib",
    "check_deps_on_npm": "rearguard check_deps_on_npm",
    "check_deps_on_npm:install": "rearguard check_deps_on_npm --install_deps",
    "prepublishOnly": "npm run build:both",
    "start": "rearguard wds",
    "start:release": "rearguard wds -r"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "rearguard": {
    "context": "src",
    "entry": "index.tsx",
    "dll_entry": "vendors.ts",
    "lib_entry": "lib_exports.ts",
    "modules": [
      "src"
    ],
    "output": {
      "path": "dist",
      "publicPath": "/"
    },
    "sync_project_deps": [],
    "has_dll": true,
    "has_browser_lib": true,
    "load_on_demand": true,
    "has_project": false,
    "has_node_lib": false,
    "publish_in_git": false
  },
  "types": "lib/lib_exports.d.ts"
}