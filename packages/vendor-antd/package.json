{
  "name": "@rtcts/vendor-antd",
  "description": "Contains all basic dependencies for browser development (lib, app)",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/mitya-borodin/base-code.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://gitlab.com/mitya-borodin/base-code/issues"
  },
  "version": "1.1.6",
  "main": "index.js",
  "bin": "",
  "files": [
    "dll_bundle"
  ],
  "scripts": {
    "build": "rearguard build",
    "sync": "rearguard sync",
    "refresh": "rearguard refresh"
  },
  "types": "",
  "dependencies": {
    "@rtcts/vendor-react": "1.1.6",
    "antd": "3.26.9",
    "tslib": "2.0.0"
  },
  "devDependencies": {
    "@types/eslint": "6.1.8",
    "@types/prettier": "1.19.0",
    "@types/stylelint": "9.10.1",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "normalize.css": "8.0.1",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "stylelint": "13.5.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-standard": "20.0.0",
    "typescript": "3.9.3"
  },
  "keywords": [],
  "engines": {
    "node": ">=10 <13",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
  },
  "rearguard": {
    "webpack": {
      "context": "src",
      "dll_entry": "rtcts_vendor_antd_vendors.ts",
      "output": {
        "path": "dist",
        "publicPath": "/",
        "libraryTarget": "var"
      }
    },
    "project": {
      "runtime": "browser",
      "type": "dll",
      "unPublishedDependency": [],
      "buildListOfLoadOnDemandModulesForAll": false
    },
    "distribution": {
      "publish_to_git": false
    }
  }
}
