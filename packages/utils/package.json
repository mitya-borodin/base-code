{
  "name": "@borodindmitriy/utils",
  "description": "Utils.",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "bugs": {
    "url": "https://gitlab.com/mitya-borodin/base-code/issues"
  },
  "version": "2.1.0",
  "main": "lib/lib_exports.js",
  "module": "lib/lib_exports.js",
  "files": [
    "lib",
    "lib_bundle"
  ],
  "scripts": {
    "build": "rearguard build --browser_lib --node_lib",
    "build:both": "rearguard build --both --browser_lib --node_lib",
    "build:release": "rearguard build -r --browser_lib --node_lib",
    "check_deps_on_npm": "rearguard check_deps_on_npm",
    "check_deps_on_npm:install": "rearguard check_deps_on_npm --install_deps",
    "prepublishOnly": "npm run build:both",
    "start": "rearguard wds",
    "start:release": "rearguard wds -r",
    "test": "rearguard test"
  },
  "types": "lib/lib_exports.d.ts",
  "dependencies": {
    "tslib": "1.10.0"
  },
  "peerDependencies": {
    "@borodindmitriy/vendors": "2.1.0"
  },
  "devDependencies": {
    "@borodindmitriy/vendors": "2.1.0",
    "@types/chai": "4.1.7",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.0",
    "chai": "4.2.0",
    "husky": "1.3.1",
    "mocha": "6.1.4",
    "prettier": "1.16.4",
    "ts-node": "8.1.0",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.15.0",
    "typescript": "3.4.1"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "rearguard": {
    "context": "src",
    "entry": "index.tsx",
    "lib_entry": "lib_exports.ts",
    "modules": [
      "src"
    ],
    "output": {
      "path": "dist",
      "publicPath": "/"
    },
    "sync_project_deps": [
      "@borodindmitriy/vendors"
    ],
    "is_application": false,
    "is_back_end": false,
    "has_dll": false,
    "has_browser_lib": true,
    "load_on_demand": false,
    "has_node_lib": true,
    "publish_in_git": false
  }
}