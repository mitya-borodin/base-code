{
  "name": "@borodindmitriy/front-end-lib",
  "description": "Front-end common classes's library.",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "bugs": {
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "version": "1.0.27",
  "main": "lib/lib_exports.js",
  "module": "lib/lib_exports.js",
  "files": [
    "dll_bundle",
    "lib",
    "lib_bundle"
  ],
  "scripts": {
    "build": "rearguard build --dll --ui_lib --node_lib",
    "build:both": "rearguard build --both --dll --ui_lib --node_lib",
    "build:release": "rearguard build -r --dll --ui_lib --node_lib",
    "dll": "rearguard build --dll",
    "dll:release": "rearguard build --dll -r",
    "project:build": "rearguard build --project",
    "project:build:release": "rearguard build --project -r",
    "prepublishOnly": "npm run build:both",
    "start": "rearguard wds",
    "start:release": "rearguard wds -r"
  },
  "dependencies": {},
  "peerDependencies": {
    "@borodindmitriy/interfaces": "1.0.13",
    "@borodindmitriy/isomorphic": "1.0.18",
    "@borodindmitriy/utils": "1.3.16",
    "@borodindmitriy/vendors": "1.0.5"
  },
  "devDependencies": {
    "@borodindmitriy/interfaces": "1.0.13",
    "@borodindmitriy/isomorphic": "1.0.18",
    "@borodindmitriy/utils": "1.3.16",
    "@borodindmitriy/vendors": "1.0.5"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "rearguard": {
    "context": "src",
    "entry": "index.tsx",
    "dll_entry": "vendors.ts",
    "lib_entry": "lib_exports.ts",
    "modules": [
      "src"
    ],
    "output": {
      "path": "dist",
      "publicPath": "/"
    },
    "sync_project_deps": [
      "@borodindmitriy/vendors",
      "@borodindmitriy/interfaces",
      "@borodindmitriy/utils",
      "@borodindmitriy/isomorphic"
    ],
    "has_project": true,
    "has_dll": true,
    "has_node_lib": true,
    "has_ui_lib": true,
    "publish_in_git": false
  },
  "types": "lib/lib_exports.d.ts"
}