{
  "name": "@borodindmitriy/mobx-vendor",
  "license": "MIT",
  "author": "Borodin Dmitriy",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "bugs": {
    "url": "https://gitlab.com/mitya-borodin/base-code/issues"
  },
  "version": "1.0.6",
  "main": "index.js",
  "files": [
    "dll_bundle"
  ],
  "scripts": {
    "build": "rearguard build --dll",
    "build:both": "rearguard build --both --dll",
    "build:release": "rearguard build -r --dll",
    "check_deps_on_npm": "rearguard check_deps_on_npm",
    "check_deps_on_npm:install": "rearguard check_deps_on_npm --install_deps",
    "prepublishOnly": "npm run build:both",
    "test": "rearguard test"
  },
  "dependencies": {
    "mobx": "5.13.0",
    "mobx-react": "5.4.3",
    "mobx-react-devtools": "6.1.1",
    "mobx-utils": "5.4.1",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.4",
    "chai": "4.2.0",
    "husky": "3.0.5",
    "mocha": "6.2.0",
    "prettier": "1.18.2",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.0.0-pre.42",
    "tslint": "5.19.0",
    "typescript": "3.6.2"
  },
  "engines": {
    "node": ">=10 <11",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "rearguard": {
    "context": "src",
    "dll_entry": "vendors.ts",
    "modules": [
      "src"
    ],
    "output": {
      "path": "dist",
      "publicPath": "/"
    },
    "sync_project_deps": [],
    "is_application": false,
    "is_back_end": false,
    "has_dll": true,
    "has_browser_lib": false,
    "load_on_demand": false,
    "has_node_lib": false,
    "publish_in_git": false
  }
}