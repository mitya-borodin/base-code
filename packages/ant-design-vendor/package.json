    "dll_bundle"
  ],
  "scripts": {
    "build": "npx rearguard build"
  },
  "types": "",
  "dependencies": {
    "@borodindmitriy/general-vendor": "1.0.2",
    "antd": "3.23.2",
    "tslib": "1.10.0"
  },
  "engines": {
    "node": ">=10 <11",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
