{
  "name": "@framework/isomorphic",
  "description": "Isomorphic part of the framework, contains basic abstractions and implementations",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:mitya-borodin/base-code.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "version": "1.0.0",
  "main": "./lib/export.js",
  "bin": "",
  "files": [
    "lib",
    "lib_bundle"
  ],
  "scripts": {
    "start": "rearguard start",
    "start:ts_node_dev": "rearguard start --ts_node_dev",
    "build": "rearguard build",
    "test": "rearguard test",
    "lint": "rearguard lint",
    "lint-fix": "rearguard lint --fix",
    "typecheck": "tsc --noEmit",
    "validate-prettiness": "prettier -c '**/*.{ts,tsx,json,md}'",
    "validate": "npm run lint && npm run typecheck && npm run validate-prettiness",
    "make-prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "postinstall": "npx typesync"
  },
  "types": "./lib/export.d.ts",
  "dependencies": {
    "@borodindmitriy/utils": "^3.0.5",
    "@borodindmitriy/vendors": "^1.0.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/eslint": "^6.1.3",
    "@types/prettier": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=10 <13",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
  },
  "rearguard": {
    "webpack": {
      "context": "src",
      "dll_entry": "vendors.ts",
      "entry": "index.tsx",
      "lib_entry": "export.ts",
      "modules": [
        "src"
      ],
      "output": {
        "path": "dist",
        "publicPath": "/"
      }
    },
    "project": {
      "runtime": "isomorphic",
      "type": "lib",
      "will_load_on_demand": false
    },
    "distribution": {
      "publish_to_git": false
    },
    "configs": {
      "noOverwriteTSConfig": false,
      "noOverwriteTSTestConfig": false,
      "noOverwriteLintConfig": false,
      "noOverwriteGitIgnore": false
    },
    "css": {
      "postcssPlugins": "postcss.config.js"
    }
  },
  "keywords": [
    "framework",
    "isomorphic",
    "entity",
    "value object",
    "library",
    "rearguard",
    "user",
    "log",
    "ws",
    "validates"
  ]
}
