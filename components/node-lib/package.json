{
  "name": "@rtcts/node-lib",
  "description": "Contains basic abstractions and implementations for creating a node application",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/mitya-borodin/base-code.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "version": "1.1.26",
  "main": "./lib/export.js",
  "bin": "",
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "rearguard start",
    "build": "rearguard build",
    "test": "rearguard test",
    "lint": "rearguard lint",
    "lint-fix": "rearguard lint --fix",
    "typecheck": "tsc --noEmit",
    "validate-prettiness": "prettier -c '**/*.{ts,tsx,json,md}'",
    "validate": "npm run lint && npm run typecheck && npm run validate-prettiness",
    "make-prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "sync": "rearguard sync",
    "refresh": "rearguard refresh"
  },
  "types": "./lib/export.d.ts",
  "dependencies": {
    "@rtcts/isomorphic": "1.1.2",
    "@rtcts/utils": "1.1.2",
    "bson": "4.0.3",
    "chalk": "3.0.0",
    "co-body": "6.0.0",
    "eventemitter3": "4.0.0",
    "file-type": "14.1.3",
    "formidable": "1.2.1",
    "jsonwebtoken": "8.5.1",
    "koa": "2.11.0",
    "koa-compress": "3.0.0",
    "koa-logger": "3.2.1",
    "koa-router": "7.4.0",
    "lodash.omit": "4.5.0",
    "moment": "2.24.0",
    "mongodb": "3.5.3",
    "ms": "2.1.2",
    "tslib": "1.10.0",
    "type-is": "1.6.18",
    "uuid": "3.4.0",
    "ws": "7.2.1"
  },
  "devDependencies": {
    "@types/bson": "1.0.11",
    "@types/chai": "4.1.7",
    "@types/co-body": "0.0.3",
    "@types/eslint": "6.1.3",
    "@types/formidable": "1.0.31",
    "@types/jsonwebtoken": "8.3.5",
    "@types/koa": "2.11.0",
    "@types/koa-compress": "2.0.9",
    "@types/koa-logger": "3.1.1",
    "@types/koa-router": "7.0.42",
    "@types/lodash.omit": "4.5.6",
    "@types/mocha": "5.2.6",
    "@types/mongodb": "3.1.22",
    "@types/ms": "0.7.31",
    "@types/node": "12.7.12",
    "@types/prettier": "1.19.0",
    "@types/type-is": "1.6.3",
    "@types/uuid": "3.4.4",
    "@types/ws": "6.0.1",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "husky": "4.2.3",
    "mocha": "7.0.1",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "typescript": "3.7.5"
  },
  "keywords": [],
  "engines": {
    "node": ">=10 <11",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
  },
  "rearguard": {
    "project": {
      "runtime": "node",
      "type": "lib",
      "unPublishedDependency": []
    },
    "distribution": {
      "publish_to_git": false
    },
    "configs": {
      "noOverwriteTSConfig": false,
      "noOverwriteTSTestConfig": false,
      "noOverwriteLintConfig": false,
      "noOverwriteGitIgnore": false
    }
  }
}
