{
  "name": "@rtcts/node-lib",
  "description": "Contains basic abstractions and implementations for creating a node application",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/mitya-borodin/base-code.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://gitlab.com/mitya-borodin/base-code"
  },
  "version": "1.0.3",
  "main": "./lib/export.js",
  "bin": "",
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "rearguard start",
    "build": "rearguard build",
    "test": "rearguard test",
    "lint": "rearguard lint",
    "lint-fix": "rearguard lint --fix",
    "typecheck": "tsc --noEmit",
    "validate-prettiness": "prettier -c '**/*.{ts,tsx,json,md}'",
    "validate": "npm run lint && npm run typecheck && npm run validate-prettiness",
    "make-prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "sync": "rearguard sync",
    "refresh": "rearguard refresh"
  },
  "types": "./lib/export.d.ts",
  "dependencies": {
    "@rtcts/isomorphic": "1.0.3",
    "@rtcts/utils": "1.0.4",
    "bson": "^4.0.2",
    "chalk": "^3.0.0",
    "eventemitter3": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "lodash.omit": "^4.5.0",
    "moment": "^2.24.0",
    "mongodb": "^3.3.5",
    "ms": "^2.1.2",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bson": "^1.0.11",
    "@types/chai": "^4.1.7",
    "@types/eslint": "^6.1.3",
    "@types/eventemitter3": "^2.0.2",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/koa": "^2.11.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.42",
    "@types/lodash.omit": "^4.5.6",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.22",
    "@types/ms": "^0.7.31",
    "@types/node": "^12.7.12",
    "@types/prettier": "^1.19.0",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "husky": "^1.3.1",
    "mocha": "^6.0.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.6.2"
  },
  "keywords": [],
  "engines": {
    "node": ">=10 <11",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
  },
  "rearguard": {
    "project": {
      "runtime": "node",
      "type": "lib",
      "unPublishedDependency": []
    },
    "distribution": {
      "publish_to_git": false
    },
    "configs": {
      "noOverwriteTSConfig": false,
      "noOverwriteTSTestConfig": false,
      "noOverwriteLintConfig": false,
      "noOverwriteGitIgnore": false
    }
  }
}
