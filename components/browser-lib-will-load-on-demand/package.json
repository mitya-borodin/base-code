{
  "name": "@rtcts/browser-lib-will-load-on-demand",
  "description": "Example of a module that can be loaded on demand",
  "license": "MIT",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "https://gitlab.com/mitya-borodin/base-code",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/mitya-borodin/base-code.git"
  },
  "bugs": {
    "email": "dmitriy@borodin.site",
    "url": "https://gitlab.com/mitya-borodin/base-code/issues"
  },
  "version": "1.0.1",
  "main": "./lib/export.js",
  "bin": "",
  "files": [
    "lib",
    "lib_bundle"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "rearguard start",
    "build": "rearguard build",
    "test": "rearguard test",
    "lint": "rearguard lint",
    "lint-fix": "rearguard lint --fix",
    "typecheck": "tsc --noEmit",
    "validate-prettiness": "prettier -c '**/*.{ts,tsx,json,md}'",
    "validate": "npm run lint && npm run typecheck && npm run validate-prettiness",
    "make-prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "postinstall": "npx typesync",
    "sync": "rearguard sync",
    "refresh": "rearguard refresh"
  },
  "types": "./lib/export.d.ts",
  "dependencies": {
    "react": "^16.12.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/eslint": "^6.1.6",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.0",
    "@types/prettier": "1.16.4",
    "@types/react": "^16.9.19",
    "@types/stylelint": "^9.10.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "4.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-react": "^7.16.0",
    "husky": "1.3.1",
    "mocha": "6.1.4",
    "normalize.css": "^8.0.1",
    "prettier": "1.16.4",
    "pretty-quick": "^1.11.1",
    "stylelint": "^13.0.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^19.0.0",
    "ts-node": "8.1.0",
    "ts-node-dev": "1.0.0-pre.32",
    "tslint": "5.15.0",
    "typescript": "3.4.1"
  },
  "keywords": [],
  "engines": {
    "node": ">=10 <13",
    "npm": ">=6 <7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run validate"
    }
  },
  "rearguard": {
    "webpack": {
      "context": "src",
      "dll_entry": "vendors.ts",
      "entry": "index.tsx",
      "lib_entry": "export.ts",
      "modules": [
        "src"
      ],
      "output": {
        "path": "dist",
        "publicPath": "/"
      }
    },
    "project": {
      "runtime": "browser",
      "type": "lib",
      "will_load_on_demand": true
    },
    "distribution": {
      "publish_to_git": false
    },
    "configs": {
      "noOverwriteTSConfig": false,
      "noOverwriteTSTestConfig": false,
      "noOverwriteLintConfig": false,
      "noOverwriteGitIgnore": false
    },
    "css": {
      "postcssPlugins": "postcss.config.js",
      "useOnlyIsomorphicStyleLoader": false
    }
  }
}
