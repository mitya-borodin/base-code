{
  "version": "1.0.0",
  "name": "rtcts",
  "scripts": {
    "bootstrap": "lerna exec \"del node_modules dist build web_modules && npm i\"",
    "fmt": "lerna run fmt",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "build": "lerna run build",
    "outdated": "lerna exec \"npm-check-updates --timeout 120000\"",
    "upgrade": "lerna exec \"npm-check-updates --timeout 120000 -u --target minor\""
  },
  "devDependencies": {
    "del-cli": "^3.0.1",
    "lerna": "^3.22.1",
    "npm-check-updates": "^11.1.1"
  }
}
